{
  "name": "baby-cry-analyzer",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:performance": "jest-performance",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm-run-all --parallel type-check lint test",
    "clean": "rimraf build",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace BabyCryAnalyzer.xcworkspace -scheme BabyCryAnalyzer -configuration Release",
    "security-scan": "npm audit && snyk test",
    "update-deps": "npm-check-updates",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.3",
    "@expo/vector-icons": "^13.0.0",
    "@opentelemetry/api": "^1.0.0",
    "@react-aria/interactions": "^3.0.0",
    "@react-native-async-storage/async-storage": "^1.17.0",
    "@react-native-firebase/analytics": "^18.0.0",
    "@react-native-firebase/messaging": "^18.0.0",
    "@react-navigation/bottom-tabs": "^6.0.0",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/react": "^7.0.0",
    "@sentry/react-native": "^5.0.0",
    "@tensorflow/tfjs": "4.2.0",
    "axios": "1.3.0",
    "axios-retry": "^3.5.0",
    "chart.js": "^4.0.0",
    "class-validator": "^0.14.0",
    "compression-streams-polyfill": "^1.0.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.0",
    "formik": "^2.2.9",
    "framer-motion": "^6.0.0",
    "i18next": "22.0.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "i18next-http-backend": "^2.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.0",
    "numeral": "^2.0.6",
    "react": "^18.0.0",
    "react-datepicker": "^4.8.0",
    "react-error-boundary": "^4.0.0",
    "react-i18next": "^11.0.0",
    "react-native": "^0.71.0",
    "react-native-circular-progress": "^1.3.9",
    "react-native-reanimated": "^2.14.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-redux": "^8.0.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "1.2.0",
    "redux-state-sync": "^3.1.0",
    "redux-thunk": "^2.4.0",
    "resize-observer-polyfill": "^1.5.1",
    "socket.io-client": "^4.6.0",
    "styled-components": "^5.3.0",
    "validator": "^13.7.0",
    "web-media-recorder": "^2.0.0",
    "worker-loader": "3.0.8",
    "ws": "^8.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@axe-core/react": "4.7.3",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@jest/globals": "29.0.0",
    "@testing-library/jest-native": "5.4.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "^12.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "axios-mock-adapter": "^1.21.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-security": "^1.7.0",
    "jest": "29.0.0",
    "jest-axe": "^7.0.0",
    "jest-performance": "1.0.0",
    "metro-config": "0.75.0",
    "metro-react-native-babel-preset": "0.75.0",
    "prettier": "^2.8.0",
    "typescript": "4.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}